<commandset label="USB251x">
	<command id="setup" info="Write all registers">
		<write reg="0x00">0x06 0x24 0x04 0x14 0x26 0xA0 0x0B</write>
		<write reg="0x06">0x01 0x9B</write> <!-- self,MTT,EOP off, individual cur sense -->
		<write reg="0x07">0x01 0xA4</write> <!-- Dynamic power, compound, 8ms overcurrent, default:20 -->
		<write reg="0x08">0x01 0x00</write>
		<write reg="0x09">0x01 0x10</write> <!-- Port 4 non-removable -->
		<write reg="0x0A">0x09 0x00 0x00 0x01 0x32 0x01 0x32 0x32 0x00 0x00</write>
		<write reg="0x13">0x03 0x04 0x06 0x02</write> <!-- Manufacturer:4B, product:6B, serial:2B -->
		<write reg="0x16">0x04 0x56 0x4C 0x49 0x5A</write> <!-- Manufacturer: VLIZ -->
		<write reg="0x54">0x06 0x44 0x41 0x53 0x48 0x55 0x42</write> <!-- Product: DASHUB -->
		<write reg="0x92">0x02 0x00 0x01</write> <!-- serial: 1 -->
		<write reg="0xFA">0x01 0x10</write> <!-- Swap D+ and D- for port 1 -->
	</command>
	<command id="start" info="Start the hub with the current settings">
		<write reg="0xFF">0x01 0x05</write> <!-- Start hub -->
	</command>
</commandset>

<write reg="0x00">0x24 0x04 0x14 0x26 0xA0 0x0B 0x9B 0xA4</write>
<write reg="0x08">0x00 0x02 0x00 0x00 0x01 0x32 0x01 0x32</write>
<write reg="0x10">0x32 0x00 0x00 0x04 0x06 0x02 0x56 0x4C</write>
<write reg="0x18">0x49 0x5A</write>
<write reg="0x54">0x44 0x41 0x53 0x48 0x55 0x42</write> <!-- Product: DASHUB -->
<write reg="0x92">0x00 0x01</write><!-- serial: 1 -->
