<commandset label="PAC1720">
	<command id="setup10ma" info="Alter the Vsense range to 10mA, 8 samples average">
		<write reg="0x0B">0x7C</write>
		<write reg="0x0C">0x7C</write>
	</command>
	<command id="setup20ma" info="Alter the Vsense range to 20mA">
		<write reg="0x0B">0x51</write>
		<write reg="0x0C">0x51</write>
	</command>
	<command id="readregs" info="Read all registers">
		<read reg="0x00" return="6"/> <!-- Configuration, conversion rate,one-shot,channel mask, hl stat,ll stat   -->		
		<read reg="0x0A" return="1"/> <!-- Vsource sampling conf -->
		<read reg="0xFD" return="1"/> <!-- Product ID -->
		<read reg="0xFE" return="1"/> <!-- Manufacturer ID -->
		<read reg="0xFF" return="1"/> <!-- Revision -->
	</command>
	<command id="raw" info="Read all raw values for source,sense and power">
		<!-- Vsense1 setup, sense, source, power -->
		<read reg="0x0B" return="14"/>
	</command>
	<command id="ch1_raw" info="Read ch1 raw values for source,sense and power">
		<!-- Vsense1 setup -->
		<read reg="0x0B" return="1"/>
		<!-- Sense Voltage -->
		<read reg="0x0D" return="2"/>
		<!-- Source Voltage -->
		<read reg="0x11" return="2"/>		
		<!-- Power Ratio -->
		<read reg="0x15" return="2"/>
	</command>
	<command id="ch2_raw" info="Read ch2 raw values for source,sense and power">
		<!-- Vsense1 setup --> 
		<read reg="0x0C" return="1"/>
		<!-- Sense Voltage -->
		<read reg="0x0F" return="2"/>
		<!-- Source Voltage -->
		<read reg="0x13" return="2"/>
		<!-- Power Ratio -->
		<read reg="0x17" return="2"/>
	</command>
</commandset>